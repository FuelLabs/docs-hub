      "postcss@<8.4.31": ">=8.4.31",
      "zod@<=3.22.2": ">=3.22.3",
      "next@<13.4.20-canary.13": ">=13.4.20-canary.13",
      "axios@>=0.8.1 <1.6.0": ">=1.6.0",
      "sharp@<0.32.6": ">=0.32.6",
      "ip@<1.1.9": ">=1.1.9",
      "ip@=2.0.0": ">=2.0.1",
      "follow-redirects@<=1.15.5": ">=1.15.6",
      "unist-util-visit@<5.0.0": "5.0.0",
      "rehype-pretty-code@>0.10.0": "0.10.0",
      "shiki@>0.14.4": "0.14.4"
    }
  }

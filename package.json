    "@mdx-js/react": "^2.3.0",
    "@tanstack/react-query": "^4.33.0",
    "@vercel/analytics": "^1.0.2",
    "acorn": "^8.10.0",
    "acorn-loose": "^8.3.0",
    "acorn-walk": "^8.2.0",
    "adm-zip": "^0.5.10",
    "airtable": "^0.12.2",
    "contentlayer": "^0.3.4",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.16.4",
    "fuels": "^0.89.1",
    "globby": "^13.2.2",

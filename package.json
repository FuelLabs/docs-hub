    "@next/mdx": "^13.4.7",
    "acorn": "^8.9.0",
    "acorn-walk": "^8.2.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.12.17",
    "fuels": "0.45.0",
    "globby": "^13.2.0",
    "gray-matter": "^4.0.3",
    "hast-util-heading-rank": "^2.1.1",
    "hast-util-to-string": "^2.0.0",
    "next": "13.4.7",
    "next-mdx-remote": "^4.4.1",
    "node-html-markdown": "^1.3.0",
    "plyr-react": "^5.3.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-aria": "^3.25.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "remark": "^14.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-slug": "^7.0.1",
    "strip-indent": "^4.0.0",

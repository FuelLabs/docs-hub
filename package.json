    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.7",
    "eslint": "^8.48.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.5",
    "typescript": "^5.2.2"
  },
  "pnpm": {
    "overrides": {
      "semver@<7.5.2": ">=7.5.2",
      "word-wrap": "npm:@aashutoshrathi/word-wrap",
      "@adobe/css-tools@<4.3.1": ">=4.3.1",
      "graphql@>=16.3.0 <16.8.1": ">=16.8.1"

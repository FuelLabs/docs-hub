    "react-aria": "^3.27.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "rehype-pretty-code": "^0.10.0",
    "remark": "^14.0.3",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-mdx": "^2.3.0",
    "remark-parse": "^10.0.2",
    "remark-slug": "^7.0.1",
    "shiki": "^0.14.4",
    "strip-indent": "^4.0.0",
    "swr": "^2.2.2",

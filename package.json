{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "node src/script.mjs && next dev",
    "build": "next build",
    "export": "DOCS_DIST=${DOCS_DIST:=dist} next export -o $DOCS_DIST",
    "deps:update": "updates -gu && pnpm -r exec updates -gu",
    "start": "next start",
    "check": "run-s lint:check prettier:check",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "pnpm lint:check --fix",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "clean:docs:build": "sh ./scripts/clean-build-files.sh",
    "sync:docs": "sh ./scripts/sync-repos.sh",
    "build:docs": "run-s sync:docs clean:docs:build build",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@docsearch/css": "^3.5.1",

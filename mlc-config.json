{
  "aliveStatusCodes": [200, 206, 405],
  "ignorePatterns": [
    {
      "pattern": "localhost:"
    },
    {
      "pattern": "^http://[1-9]"
    },
    {
      "pattern": "crates\\.io"
    },
    {
      "pattern": "infura\\.io"
    },
    {
      "pattern": "\\.zip$"
    },
    {
      "pattern": "-indexer.fuel.network"
    },
    {
      "pattern": "fuellabs.github.io/block-explorer-v2"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^(/)(.*)",
      "replacement": "http://localhost:3030/$2"
    }
  ]
}
